<p style="color: green"><%= notice %></p>

<%= render @tweet %>

<div>
<br>
  <%= link_to "Editar", edit_tweet_path(@tweet), :class => "btn btn-outline-primary" %> |
  <%= link_to "Volver", tweets_path, :class => "btn btn-outline-primary" %>
  <hr>
  <%= button_to "Destroy this tweet", @tweet, method: :delete, :class => "btn btn-danger" %>
</div>
